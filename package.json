{
  "name": "mozy",
  "version": "0.5.2",
  "description": "A model library using lazy initialization, factories and instance registries.",
  "type": "commonjs",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "es6-error": "^4.1.1",
    "eventemitter3": "^5.0.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.5",
    "@babel/register": "^7.28.3",
    "@eslint/js": "^9.39.1",
    "c8": "^10.1.3",
    "chai": "^6.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-mocha": "^11.2.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-promise": "^7.2.1",
    "globals": "^16.5.0",
    "lodash.uniq": "^4.5.0",
    "mocha": "^11.7.5",
    "rimraf": "^6.1.0",
    "typescript": "^5.9.3"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir dist/cjs --copy-files --source-maps inline",
    "build:esm": "cross-env BABEL_ENV=esm babel src --out-dir dist/esm --copy-files --source-maps inline",
    "build:types": "tsc --project tsconfig.types.json",
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "watch": "cross-env BABEL_ENV=cjs babel src --watch --out-dir dist/cjs --copy-files",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "release": "npm run lint && npm test && npm run build",
    "test": "npm run test:coverage",
    "test:unit": "cross-env BABEL_ENV=cjs mocha -r @babel/register ./test/**/*.test.js",
    "test:coverage": "c8 --reporter=text --reporter=lcov npm run test:unit",
    "lint": "eslint src/**.js test/**.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/insector-ab/mozy.git"
  },
  "keywords": [
    "model",
    "lazy",
    "factory",
    "registry",
    "mozy"
  ],
  "author": "Martin Söderholm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/insector-ab/mozy/issues"
  },
  "homepage": "https://github.com/insector-ab/mozy#readme",
  "contributors": [
    {
      "name": "Martin Söderholm",
      "email": "matte@insector.se"
    },
    {
      "name": "Joel Källgren",
      "email": "joel@insector.se"
    }
  ]
}
